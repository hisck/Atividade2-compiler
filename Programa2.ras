program procVolumeParalelepipedoRetangular;
    var lado1, lado2, altura: integer;

    procedure volume(lado1, lado2, altura: integer);
    var volume: integer;
    begin
        volume := lado1 * lado2 * altura;
        if(volume > 0) then
        begin
            writeln(volume);
            write(true);
        end
        else
        begin
            write(false);
        end;
    end;

begin
    read(lado1);
    read(lado2);
    read(altura);
    volume(lado1, lado2, altura);
end.